{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    { "source": "/(.*)", "headers": [ { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" } ] }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/1waion.CAVEMINES/$1" }
  ]
}

